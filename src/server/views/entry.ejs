<!DOCTYPE html>
<html>
<head>
<% include ./metadata %>
<% include ./favicon_header %>
<title><%=metadata.pageTitle%></title>
<link rel="stylesheet" href="/third-party/bootstrap/css/bootstrap.min.css">
<link rel="stylesheet" href="/third-party/font-awesome/css/fontawesome-all.min.css">
<link rel="stylesheet" type="text/css" href="/css/styles.min.css">
<!-- Global site tag (gtag.js) - Google Analytics -->
<script async src="https://www.googletagmanager.com/gtag/js?id=UA-46129625-3"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'UA-46129625-3');
</script>
</head>
<body>
<% include ./header %>

<div class="container-fluid">
	<div class="row">
	<div class="col-md-1">
	</div>
	<div class="col-md-2 col-md-offset-1 fixed">
		<% include ./shortcutsSidebar %>
		<% include ./entriesSidebar %>
	</div>
	<div class="col-md-5 col-md-offset-2">
		<div id="main">
			<h1><%=entry.caption%></h1>
			<img src="<%=metadata.imageURL%>">
		</div>
		<hr/>
		<div id="tabs">
		</div>
	</div>
	<div class="col-md-2 col-md-offset-8 fixed">
		<div id="rightTopSidebar">
		</div>
		<div id="rightMiddleSidebar">
		</div>
	</div>
	<div class="col-md-1 fixed col-md-offset-10">
	</div>
	<div class="col-md-1">
	</div>
</div>


<script>
	var metadata = <%- JSON.stringify(metadata) %>;
	var entry = <%- JSON.stringify(entry) %>;
	var user = <%- JSON.stringify(user) %>;
</script>
<script src="/third-party/jquery/js/jquery.min.js"></script>
<script src="/third-party/bootstrap/js/bootstrap.min.js"></script>
<script src="/js/entry.min.js"></script>
<script src="/js/header.min.js"></script>
<script src="/js/utils.min.js"></script>
<script src="/js/containers.min.js"></script>
</body>
</html>