{
	"parserOptions" : {
		"ecmaVersion" : "6",
		"sourceType" : "module"
	},
	"env" : {
		"node" : true,
		"es6" : true
	},
	"rules" : {
		"no-empty" : "error",
		"no-multiple-empty-lines" : "warn",
		"no-var" : "warn",
		"prefer-const" : "warn",
		"no-mixed-spaces-and-tabs" : "off"
	},
	"extends" : "eslint:recommended"
}